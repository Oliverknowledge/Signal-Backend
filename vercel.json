{
  "functions": {
    "api/opik-log.ts": {
      "maxDuration": 10
    },
    "api/feedback.ts": {
      "maxDuration": 10
    },
    "api/recall.ts": {
      "maxDuration": 10
    },
    "api/grade-recall.ts": {
      "maxDuration": 30
    },
    "api/analyze.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
